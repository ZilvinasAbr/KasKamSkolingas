{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}\\src\\index.js",
      "cwd": "${workspaceRoot}",
      "sourceMaps": true,
      "outDir": "${workspaceRoot}\\dist"
    },
    {
      "name": "Attach to Process",
      "type": "node2",
      "request": "attach",
      "port": 9229
    }
  ]
}